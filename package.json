{
  "name": "@arianee/arianeejs",
  "version": "1.52.0",
  "main": "dist/src/index.js",
  "browser": "dist/src/index.js",
  "unpkg": "browser/bundle.js",
  "scripts": {
    "start": "./node_modules/.bin/tsc --watch",
    "launch": "nodemon ./dist/example/myexample.js",
    "test:watch": "./node_modules/.bin/jest --watch",
    "test:e2e": "./node_modules/.bin/cucumber-js features/implemented/**/*.feature --require 'dist/features/steps/*.step.js' --parallel 2 --fail-fast",
    "test:rerunFailTest": "./node_modules/.bin/cucumber-js @rerun.txt --require 'dist/features/steps/*.step.js'",
    "test:e2e-pretty": "./node_modules/.bin/cucumber-js features/implemented/**/*.feature --require 'dist/features/steps/*.step.js' --format node_modules/cucumber-pretty",
    "test:e2e-dev": "./node_modules/.bin/cucumber-js features/implemented/**/*.feature --require 'dist/features/steps/*.step.js' --format node_modules/cucumber-pretty --tags @dev",
    "test:unit": "./node_modules/.bin/jest --forceExit",
    "test": "gitool -cl=3 && npm run lint && npm run test:unit &&npm run test:e2e",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.json 'src/core/**/*.ts'",
    "lint:fix": "./node_modules/.bin/eslint -c .eslintrc.json --fix 'src/core/**/*.ts'",
    "doc": "./node_modules/.bin/typedoc  --out doc/ src/ --mode file --ignoreCompilerErrors --includeDeclarations --excludeExternals",
    "prepare": "npm run build",
    "prepare:publish": "npm run build && npm run test",
    "build": "npm run build-web && npm run build-node",
    "build-web": "parcel build ./src/index.ts --out-dir=browser --out-file=bundle.js --global=ArianeeLib && npm run build-webFileForTest",
    "build-webFileForTest": "parcel build ./example/myexecfile.ts --out-dir=dist",
    "build-node": "./node_modules/.bin/tsc",
    "changelog": "gt -po=changelog.md"
  },
  "files": [
    "dist",
    "browser"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Arianee/arianeeJS"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cucumber": "^4.0.5",
    "@types/jest": "^24.0.17",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "chai": "^4.2.0",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.2",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gitool": "^1.13.0",
    "jest": "^24.1.0",
    "jest-environment-node": "^26.3.0",
    "node-fetch-polyfill": "^2.0.6",
    "nodemon": "^1.19.1",
    "parcel-bundler": "^1.12.4",
    "puppeteer": "^1.20.0",
    "puppeteer-firefox": "^0.5.0",
    "source-map-loader": "^0.2.4",
    "ts-generator": "0.0.8",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.4.5",
    "tslint": "^5.20.0",
    "typechain": "^0.3.14",
    "typedoc": "^0.14.2",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "@0xcert/cert": "1.5.2",
    "@0xcert/conventions": "^2.0.0-alpha3",
    "@arianee/arianee-abi": "^0.13.0",
    "@arianee/iso-language-picker": "^0.3.0",
    "@ethereumjs/tx": "^3.1.3",
    "@types/js-base64": "^2.3.1",
    "append-query": "^2.1.0",
    "axios": "^0.21.1",
    "ethers": "^4.0.38",
    "eventemitter3": "^4.0.0",
    "js-base64": "^2.5.2",
    "promise.prototype.finally": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^3.4.0",
    "web3": "1.2.2"
  }
}
