{
  "name": "@arianee/arianeejs",
  "version": "0.9.0",
  "main": "dist/src/index.js",
  "browser": "dist/src/index.js",
  "unpkg": "browser/bundle.js",
  "scripts": {
    "start": "./node_modules/.bin/tsc --watch",
    "launch": "nodemon ./dist/example/myexample.js",
    "test:watch": "./node_modules/.bin/jest --watch",
    "test:e2e": "./node_modules/.bin/cucumber-js features/implemented/**/*.feature --require 'dist/features/steps/*.step.js' --format node_modules/cucumber-pretty",
    "test:e2e-dev": "./node_modules/.bin/cucumber-js features/dev/**/*.feature --require 'dist/features/steps/*.step.js' --format node_modules/cucumber-pretty",
    "test:unit": "./node_modules/.bin/jest",
    "test": "npm run lint && npm run test:e2e && npm run test:unit",
    "lint": "./node_modules/.bin/tslint -c tslint.json  -t verbose --fix 'src/core/**/*.ts'",
    "doc": "./node_modules/.bin/typedoc  --out doc/ src/ --mode file --ignoreCompilerErrors --includeDeclarations --excludeExternals",
    "prepare": "npm run build",
    "prepare:publish": "npm run build && npm run test",
    "build": "npm run build-web && npm run build-node",
    "build-web": "webpack -p --mode production --env browser",
    "build-node": "./node_modules/.bin/tsc"
  },
  "files": [
    "dist",
    "browser"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Arianee/arianeeJS"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@types/chai": "^4.1.7",
    "@types/cucumber": "^4.0.5",
    "@types/jest": "^24.0.17",
    "@types/web3": "^1.0.19",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^4.6.0",
    "cucumber": "^5.1.0",
    "jest": "^24.1.0",
    "nodemon": "^1.19.1",
    "puppeteer": "^1.20.0",
    "source-map-loader": "^0.2.4",
    "ts-generator": "0.0.8",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.4.5",
    "tslint": "^5.20.0",
    "typechain": "^0.3.14",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^2.0.8",
    "typescript": "^3.6.3",
    "web3-eth-contract": "^1.2.1",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@0xcert/cert": "1.5.2",
    "@0xcert/conventions": "^2.0.0-alpha3",
    "@arianee/arianee-abi": "0.3.0",
    "axios": "^0.18.1",
    "bignumber.js": "^8.0.2",
    "cucumber-pretty": "^1.5.2",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "ethers": "^4.0.23",
    "jayson": "^3.0.2",
    "node-fetch-polyfill": "^2.0.6",
    "web3": "1.2.1"
  }
}
